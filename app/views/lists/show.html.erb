<h1>Lists#show</h1>
<p>Find me in app/views/lists/show.html.erb</p>

<%= @list.name%>

<% @list.movies.each do |movie| %>
<li><%= movie.title%></li>
<li><%= movie.overview%></li>
<li><%= movie.rating%></li>
<%end%>

<%= link_to "delete this bookmark", list_path(@list), class: "btn btn-cancel", data: { turbo_method: :delete, turbo_confirm:
 "Are you sure you want to delete this list?" } %>
